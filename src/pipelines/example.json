{
  "id": "example-pipeline",
  "name": "多行合并",
  "description": "检测PDF复制出来的多行文本，自动合并成一行。",
  "tools": [
    {
      "name": "TrimWhitespace"
    },
    {
      "name": "FullwidthToHalfwidth",
      "toLowerCase": true
    },
    {
      "name": "MergeSplitLines",
      "widthRangeRatio": 0.1,
      "minLineCount": 3,
      "maxWidth": 800
    }
  ]
} 